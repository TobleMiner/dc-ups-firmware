<html>
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>DC UPS</title>
	{{include,file=resources.inc}}
	<script src="/index.js" type="text/javascript"></script>
</head>
<body>
    {{include,file=navbar.thtml}}
    <div class="container mt-2">
      <div class="row">
        <div class="col d-flex align-items-stretch">
          <div class="card mb-3 w-100">
            <div class="card-body d-flex flex-column">
              <h5 class="card-title">System</h5>
              <table class="table table-borderless mb-1">
                <tbody>
                  <tr>
                    <td>WiFi:</td>
                    <td class="js-wifi-status">--</td>
                  </tr>
                  <tr>
                    <td>Ethernet:</td>
                    <td class="js-ethernet-status">--</td>
                  </tr>
                  <tr>
                    <td>Uptime:</td>
                    <td class="js-uptime">--</td>
                  </tr>
                </tbody>
              </table>
            </div>
          </div>
        </div>
        {{include,file=status_panel.thtml,status_panel_title=DC input,status_panel_id=dcin}}
      </div>
      <div class="row">	
        {{include,file=status_panel.thtml,status_panel_title=DC output,status_panel_id=dcout}}
        {{include,file=status_panel.thtml,status_panel_title=USB output,status_panel_id=usbout}}
      </div>
    </div>
</body>
</html>
